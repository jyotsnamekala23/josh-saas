<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
      <display-name>josh-saas</display-name>
      <distributable></distributable>
      
      
    <servlet>
         <servlet-name>JoshSaasServlet</servlet-name>
         <servlet-class>com.web.josh.servlet.JoshSaasServlet</servlet-class>
         <load-on-startup>0</load-on-startup>
     </servlet>
     
  
  <servlet-mapping>
       <servlet-name>JoshSaasServlet</servlet-name>
       <url-pattern>/JoshSaasServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
 </welcome-file-list>
 
 </web-app>